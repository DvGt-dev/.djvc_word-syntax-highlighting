{
  "scopeName": "source.djvc",
  "patterns": [
    {
      "name": "punctuation.period.djvc",
      "match": "\\."
    },
    {
      "name": "punctuation.comma.djvc",
      "match": ","
    },
    {
      "name": "punctuation.2pts.djvc",
      "match": ":"
    },
    {
      "name": "punctuation.apostrph.djvc",
      "match": "\\#"
    },
    {
      "name": "punctuation.parenthesis.djvc",
      "match": "\\d"
    }
  ],
  "repository": {},
  "fileTypes": ["djvc"],
  "name": "DJVC"
}
